[Nemo Action]
Active=true
Name=Install Fonts
Name[ko]=폰트 설치하기
Comment=Install selected fonts
Comment[ko]=선택한 모든 폰트 설치
Exec=gnome-terminal --working-directory=%P -- /bin/bash -c '/usr/bin/cp %F ~/.local/share/fonts'
Icon-Name=font-x-generic
Selection=any
Extensions=ttf;ttc;otf;
Mimetypes=font/otf;font/ttf;application/vnd.ms-opentype;font/sfnt;font/woff;font/woff2;
#EscapeSpaces=true
Terminal=false
Quote=double