[Nemo Action]
# Use GPrename (apt install gprename) to open parent folder of selected files for bulk rename.
Name=_Bulk Rename
Name[ko]=전체 파일 이름 바꾸기
Comment=Open GPrename in %P
Comment=선택된 파일 %P 이름 일괄 변환
Exec=gprename %P
Icon-Name=document-edit-symbolic
#Stock-Id=
Selection=m
Extensions=any;
#Mimetypes=
#Separator= 
#Quote=
Dependencies=gprename;
#Conditions=
EscapeSpaces=true
